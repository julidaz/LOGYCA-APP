<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.logyca.RegisterActivity$PlaceholderFragment" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <!-- Header  Starts -->

        <LinearLayout
            android:id="@+id/header"
            android:layout_width="fill_parent"
            android:layout_height="110dp"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/logo"
                android:layout_width="wrap_content"
                android:layout_height="94dp"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="5dp"
                android:clickable="false"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:maxWidth="@dimen/activity_horizontal_margin"
                android:src="@drawable/logo" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="120dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/titulo_intereses"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="20dp"
                android:text="@string/intereses"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="@color/gray" />

            <ScrollView
                android:id="@+id/scrollView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <LinearLayout
                    android:id="@+id/layoutIntereses"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:baselineAligned="false"
                    android:orientation="vertical"
                    android:paddingLeft="1dp" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/codigoDeBarrasIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/barcode" />

                        <TextView
                            android:id="@+id/codigoDeBarrasTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Identificación de productos por código de barras" />

                        <Switch
                            android:id="@+id/switch1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/sincronizacionInformacionIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/bars" />

                        <TextView
                            android:id="@+id/sincronizacionInformacionTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Sincronización de la información (logyca/sync)" />

                        <Switch
                            android:id="@+id/switch2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/calidadInformacionIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/signup" />

                        <TextView
                            android:id="@+id/calidadInformacionTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Calidad de la información (Data Quality)" />

                        <Switch
                            android:id="@+id/switch3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/trazabilidadMedianteEstandaresIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/tab" />

                        <TextView
                            android:id="@+id/trazabilidadMedianteEstandaresTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Trazabilidad mediante estándares" />

                        <Switch
                            android:id="@+id/switch4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/sostenibilidadIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/redo" />

                        <TextView
                            android:id="@+id/sostenibilidadTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Sostenibilidad" />

                        <Switch
                            android:id="@+id/switch5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/finanzasCadenaValorIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/coin" />

                        <TextView
                            android:id="@+id/finanzasCadenaValorTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Finanzas en la cadena de valor" />

                        <Switch
                            android:id="@+id/switch6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/riesgoCadenaValorIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/cogs" />

                        <TextView
                            android:id="@+id/riesgoCadenaValorTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Riesgo en la cadena de valor" />

                        <Switch
                            android:id="@+id/switch7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/distribucionUrbanaMercanciasIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/truck" />

                        <TextView
                            android:id="@+id/distribucionUrbanaMercanciasTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Distribución urbana de mercancías" />

                        <Switch
                            android:id="@+id/switch8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/gestionDemandaIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/bars2" />

                        <TextView
                            android:id="@+id/gestionDemandaTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Gestión de la demanda" />

                        <Switch
                            android:id="@+id/switch9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/gestionInventariosIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/books" />

                        <TextView
                            android:id="@+id/gestionInventariosTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Gestión de inventarios" />

                        <Switch
                            android:id="@+id/switch10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/gestionDistribucionTransporteIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/road" />

                        <TextView
                            android:id="@+id/gestionDistribucionTransporteTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Gestión de distribución y transporte" />

                        <Switch
                            android:id="@+id/switch11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal" >

                        <ImageView
                            android:id="@+id/disenoBodegasCentroDistribucionIv"
                            android:layout_width="55dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/office" />

                        <TextView
                            android:id="@+id/disenoBodegasCentroDistribucionTv"
                            android:layout_width="132dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:paddingLeft="10dp"
                            android:text="Diseño de bodegas/Centro de distribución" />

                        <Switch
                            android:id="@+id/switch12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:paddingTop="15dp" />
                    </LinearLayout>
                </LinearLayout>
            </ScrollView>

            <Button
                android:id="@+id/btn_guardar"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Guardar"
                android:textColor="@color/orange"
                android:textSize="23sp" />
        </LinearLayout>
        <!-- Header Ends -->

    </RelativeLayout>

</ScrollView>